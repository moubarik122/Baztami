<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- View background color -->
    <solid
        android:color="#FFAB40" >
    </solid>

    <!-- View border color and width -->
    <stroke
        android:width="3dp"
        android:color="#FF6E40" >
    </stroke>

    <!-- If padding is desired -->
    <padding
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp"    >
    </padding>

    <!-- Corner radius -->
    <corners
        android:radius="60dp"   >
    </corners>

</shape>